Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"notSoAwesomeComputationWithTempFile", "[", "max_", "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"file", "=", 
     RowBox[{"CreateFile", "[", "]"}]}], ";", 
    RowBox[{"OpenWrite", "[", "file", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"sum", "=", "0"}], "}"}], ",", 
      RowBox[{
       RowBox[{"Do", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Pause", "[", "1", "]"}], ";", 
          RowBox[{"Write", "[", 
           RowBox[{"file", ",", "i"}], "]"}], ";", 
          RowBox[{"sum", "+=", "i"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "max"}], "}"}]}], "]"}], ";", "sum"}]}], "]"}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"Close", "[", "file", "]"}], ";", 
    RowBox[{"DeleteFile", "[", "file", "]"}]}], "\[IndentingNewLine]", 
   ")"}]}], "\n", 
 RowBox[{
  RowBox[{"TimeConstrained", "[", 
   RowBox[{
    RowBox[{"notSoAwesomeComputationWithTempFile", "[", "3", "]"}], ",", 
    "1"}], "]"}], ";"}], "\n", 
 RowBox[{"FileExistsQ", "[", "file", "]"}]}], "Input",
 CellChangeTimes->{{3.8229695502813883`*^9, 3.822969583567614*^9}, 
