Cell["\<\
The following function uses a temporary file to perform an amazing \
computation, namely pause for a second and then add an integer to a sum. It \
uses WithCleanup to ensure that the file is always opened at the beginning \
and closed and erased at the end:\
\>", "Text",
 CellChangeTimes->{{3.8229691802305193`*^9, 3.8229691820704727`*^9}, {
   3.822969624957675*^9, 3.822969762384323*^9}, 3.8230043298913193`*^9, {
   3.823006059402359*^9, 3.823006070994218*^9}, {3.823030406156602*^9, 
   3.823030407988442*^9}},ExpressionUUID->"bb205e48-d411-4bbb-b518-\
0e1ee4865277"],

Cell[BoxData[
 RowBox[{
  RowBox[{"awesomeComputationWithTempFile", "[", "max_", "]"}], ":=", 
  RowBox[{"WithCleanup", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"file", "=", 
      RowBox[{"CreateFile", "[", "]"}]}], ";", 
     RowBox[{"OpenWrite", "[", "file", "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"sum", "=", "0"}], "}"}], ",", 
      RowBox[{
       RowBox[{"Do", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Pause", "[", "1", "]"}], ";", 
          RowBox[{"Write", "[", 
           RowBox[{"file", ",", "i"}], "]"}], ";", 
          RowBox[{"sum", "+=", "i"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "max"}], "}"}]}], "]"}], ";", "sum"}]}], "]"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Close", "[", "file", "]"}], ";", 
     RowBox[{"DeleteFile", "[", "file", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.822969035212069*^9, 3.8229691323288603`*^9}, {
  3.822969251012748*^9, 3.822969358128007*^9}, {3.822969390623128*^9, 
  3.822969418062025*^9}, {3.822969493724331*^9, 3.8229695174104757`*^9}},
 CellLabel->"In[43]:=",ExpressionUUID->"a23d1d57-da58-4ffc-9436-7cfc017de27e"],

