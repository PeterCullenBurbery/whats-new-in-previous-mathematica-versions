a0a93b933d0d"],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "args___", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", "sep", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"sep", "=", 
      RowBox[{"ArgumentsOptions", "[", 
       RowBox[{
        RowBox[{"f", "[", "args", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", "2"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "[", "sep", "]"}], "/;", 
      RowBox[{"!", 
       RowBox[{"FailureQ", "[", "sep", "]"}]}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
