Cell["\<\
The following function opens a named file and adds the values of all the \
bytes it contains. It uses ConfirmMatch to test the file was opened \
successfully and ConfirmAssert to test that each individual byte was \
successfully read:\
\>", "Text",
 CellChangeTimes->{{3.8229662403418417`*^9, 3.8229662635813627`*^9}, {
  3.822966324962846*^9, 3.822966393144459*^9}, {3.8230050904029493`*^9, 
  3.823005120010241*^9}},ExpressionUUID->"63f68459-890b-4e38-a298-\
31d1bb9a2ad8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"byteSum", "[", "file_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"stream", ",", "nextByte", ",", 
      RowBox[{"sum", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Enclose", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"stream", "=", 
       RowBox[{"ConfirmMatch", "[", 
        RowBox[{
         RowBox[{"OpenRead", "[", 
          RowBox[{"file", ",", 
           RowBox[{"BinaryFormat", "\[Rule]", "True"}]}], "]"}], ",", 
         "_InputStream"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"nextByte", "=", 
       RowBox[{"BinaryRead", "[", 
        RowBox[{"stream", ",", "\"\<Byte\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"nextByte", " ", "=!=", " ", "EndOfFile"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"ConfirmAssert", "[", 
          RowBox[{
           RowBox[{"IntegerQ", "[", "nextByte", "]"}], "&&", 
           RowBox[{"0", "\[LessEqual]", "nextByte", "\[LessEqual]", "255"}]}],
           "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"sum", " ", "+=", "nextByte"}], ";", "\[IndentingNewLine]", 
         RowBox[{"nextByte", "=", 
          RowBox[{"BinaryRead", "[", 
           RowBox[{"stream", ",", "\"\<Byte\>\""}], "]"}]}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "sum"}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

