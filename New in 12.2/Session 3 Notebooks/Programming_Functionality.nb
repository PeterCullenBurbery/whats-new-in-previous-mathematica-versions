Cell["\<\
The following function interprets an input string as a particle name and \
returns its mass, if known. It uses the second argument of Enclose to \
determine which Confirm fired and customize the generated failure object:\
\>", "Text",
 CellChangeTimes->{{3.822966702852377*^9, 3.82296671746807*^9}, {
  3.822966783161766*^9, 3.8229668226076803`*^9}, {3.8229668582310133`*^9, 
  3.822966915068281*^9}, {3.8230058748180847`*^9, 
  3.823005892279909*^9}},ExpressionUUID->"9ffce443-73d3-417c-a992-\
bd5519408438"],

Cell[BoxData[
 RowBox[{
  RowBox[{"particleMass", "[", "name_", "]"}], ":=", 
  RowBox[{"Enclose", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"particle", "=", 
      RowBox[{"Confirm", "[", 
       RowBox[{
        RowBox[{"Interpreter", "[", "\"\<Particle\>\"", "]"}], "[", "name", 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Confirm", "[", 
      RowBox[{"particle", "[", "\"\<Mass\>\"", "]"}], "]"}]}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
