Cell[TextData[{
 "Propagate the solution of \[ImaginaryI] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "u"}], 
     RowBox[{"\[PartialD]", "t"}]], "=", 
    RowBox[{"2", "|", "u", 
     SuperscriptBox["|", "2"], 
     RowBox[{"u", "+", 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], "u"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "2"]}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "af91d211-835c-4c6e-998d-66cafb6cc4f6"],
 " using a splitting\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"A", "+", "B"}]], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        FractionBox["A", "2"]], 
       SuperscriptBox["\[ExponentialE]", "B"], 
       SuperscriptBox["\[ExponentialE]", 
        FractionBox["A", "2"]]}], "+", 
      RowBox[{"O", "(", 
       SuperscriptBox["\[CapitalDelta]t", "2"], ")"}]}], ":"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "25b67de4-d6d5-4fc3-b851-5e9bf7a73d74"]
}], "Text",
 CellChangeTimes->{{3.8674289355174103`*^9, 
  3.867429053722884*^9}},ExpressionUUID->"fedb24ca-4fdb-4a00-a461-\
996362adcadf"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Timing", "[", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[CapitalDelta]t", "=", "0.05"}], ",", "m"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"m", "=", 
      RowBox[{"\[CapitalDelta]t", 
       RowBox[{"(", 
        RowBox[{"0.", "+", 
         RowBox[{"1.", "I"}]}], ")"}], "s"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"u", "=", 
         RowBox[{
          RowBox[{"Exp", "[", 
           RowBox[{
           "\[CapitalDelta]t", " ", "I", " ", "u", "\[IndentingNewLine]", 
            RowBox[{"Conjugate", "[", "u", "]"}]}], "]"}], "u"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"u", "=", 
         RowBox[{"MatrixExp", "[", 
          RowBox[{"m", ",", "u"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"u", "=", 
         RowBox[{
          RowBox[{"Exp", "[", 
           RowBox[{"\[CapitalDelta]t", " ", "I", " ", "u", " ", 
            RowBox[{"Conjugate", "[", "u", "]"}]}], "]"}], "u"}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"1", "/", "\[CapitalDelta]t"}], "}"}]}], "]"}]}]}], "]"}], 
  "]"}]], "Input",
