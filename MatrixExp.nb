  RowBox[{"L", "=", "10"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "500"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x", "=", 
   RowBox[{"L", "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", 
       RowBox[{
        RowBox[{"N", "[", 
         RowBox[{"Range", "[", "n", "]"}], "]"}], "/", "n"}]}], "-", "1"}], 
     ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"s", "=", 
  RowBox[{"N", "[", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"h", "=", 
       RowBox[{"2.", 
        RowBox[{"L", "/", "n"}]}]}], "}"}], ",", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["h", "2"]], 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"i_", ",", "i_"}], "}"}], "\[Rule]", 
           RowBox[{"-", "2"}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"i", "-", "j"}], "]"}], "\[Equal]", "1"}]}], "\[Rule]", 
           "1"}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"1", ",", "n"}], "}"}], "\[Rule]", "1"}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"n", ",", "1"}], "}"}], "\[Rule]", "1"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "n"}], "}"}]}], "]"}]}]}], "]"}], 
   "]"}]}]}], "Input",
