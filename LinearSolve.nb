        RowBox[{"1.", "-", 
         FractionBox["2", 
          SuperscriptBox["h", "2"]]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
