Cell[TextData[{
 "Approximately solve the boundary value problem\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["u", "xx"], "+", "u"}], "=", 
    RowBox[{
     RowBox[{"f", ":", " ", 
      RowBox[{"u", "(", "0", ")"}]}], "=", 
     RowBox[{
      RowBox[{"u", "(", "1", ")"}], "=", "0"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
